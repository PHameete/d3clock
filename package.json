    "d3-time": "^1.0.8",
    "d3-time-format": "^2.1.1",
    "d3-transition": "^1.1.1"
  },
  "devDependencies": {
    "@types/d3-ease": "^1.0.7",
    "@types/d3-selection": "^1.3.1",
    "@types/d3-time": "^1.0.8",
    "@types/d3-time-format": "^2.1.0",
